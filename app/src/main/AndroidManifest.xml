<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsapps.dota2guessthesound">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dsapps.dota2guessthesound.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dsapps.dota2guessthesound.StartQuizActivity"
            android:parentActivityName="com.dsapps.dota2guessthesound.MainActivity"
            android:theme="@style/playGameTheme" />
        <activity android:name="com.dsapps.dota2guessthesound.ToastActivity" />
        <activity android:name="com.dsapps.dota2guessthesound.OptionsActivity" />
        <activity
            android:name="com.dsapps.dota2guessthesound.FastFingerActivity"
            android:theme="@style/playGameTheme" />
        <activity android:name="com.dsapps.dota2guessthesound.InvokerActivity"
            android:theme="@style/playGameTheme"/>
    </application>

</manifest>